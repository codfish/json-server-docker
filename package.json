{
  "name": "json-server-docker",
  "version": "0.0.0-semantically-released",
  "private": true,
  "scripts": {
    "start": "./server.sh",
    "dev": "nodemon -w db.js -w middleware.js -w routes.json --exec ./server.sh",
    "format": "cod-scripts format",
    "lint": "cod-scripts lint",
    "test": "cod-scripts test"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "faker": "git+https://git@github.com/Marak/faker.js.git#a9a11ee",
    "lodash": "^4.17.11",
    "nodemon": "^1.19.1"
  },
  "devDependencies": {
    "@commitlint/travis-cli": "^8.0.0",
    "cod-scripts": "^4.0.1"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/cod-scripts/eslint.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "cod-scripts pre-commit",
      "commit-msg": "cod-scripts commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
