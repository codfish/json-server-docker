{
  "name": "json-server-docker",
  "version": "0.0.0-semantically-released",
  "private": true,
  "scripts": {
    "start": "./server.sh",
    "dev": "nodemon -w db.js -w middleware.js -w routes.json --exec ./server.sh",
    "fix": "npm run format && npm run lint -- --fix",
    "format": "prettier --write \"**/*.{json,css,scss,md}\"",
    "lint": "eslint .",
    "lint:md": "markdownlint -i node_modules .",
    "validate": "commitlint-travis && npm run lint && npm run lint:md"
  },
  "dependencies": {
    "faker": "git+https://git@github.com/Marak/faker.js.git#a9a11ee",
    "lodash": "^4.17.11",
    "nodemon": "^1.19.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/travis-cli": "^8.0.0",
    "eslint": "^5.15.2",
    "eslint-config-codfish": "^2.3.0",
    "husky": "^2.4.0",
    "lint-staged": "^10.0.5",
    "markdownlint-cli": "^0.16.0",
    "prettier": "^1.16.4"
  }
}
