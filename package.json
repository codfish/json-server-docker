{
  "name": "json-server-docker",
  "version": "0.0.0-semantically-released",
  "private": true,
  "scripts": {
    "start": "json-server -w --fks Address --id address --host 0.0.0.0 --routes routes.json --middlewares middleware.js -p 80 index.js",
    "dev": "nodemon -w index.js -w middleware.js -w routes.json --exec npm start",
    "fix": "npm run format && npm run lint -- --fix",
    "format": "prettier --write \"**/*.{json,css,scss,md}\"",
    "lint": "eslint .",
    "validate": "commitlint-travis && npm run lint"
  },
  "dependencies": {
    "faker": "git+https://git@github.com/Marak/faker.js.git#d3ce6f1",
    "json-server": "^0.14.2",
    "lodash": "^4.17.11",
    "nodemon": "^1.18.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@commitlint/travis-cli": "^7.5.2",
    "eslint": "^5.15.2",
    "eslint-config-codfish": "^2.3.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4"
  }
}
